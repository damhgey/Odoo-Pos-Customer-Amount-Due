<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="ClientListScreen" t-inherit="point_of_sale.ClientListScreen" t-inherit-mode="extension" owl="1">
        <xpath expr="//div[hasclass('top-content')]" position="inside">
             <div class="button back" t-on-click="getAmountDueClients()">
                 <span class="icon"> <i class="fa fa-money"/> </span>
                 <span class="load-customer-search"> Unpaid </span>
             </div>
        </xpath>
    </t>

 </templates>
